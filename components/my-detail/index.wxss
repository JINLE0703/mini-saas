.container {
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.user {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 346rpx;
}

.avatar {
  width: 162rpx;
  height: 162rpx;
  margin-bottom: 30rpx;
  background-color: #C9DDF8;
  border-radius: 50%;
}

.name {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 60rpx;
  line-height: 60rpx;
}

.name-content {
  margin-right: 20rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #262626;
}

.iconfont {
  font-family: 'iconfont' !important;
}

.arrow-right {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  font-weight: bold;
}

.arrow-right::before {
  content: '\e688';
}

.reserve {
  margin-bottom: 24rpx;
}

.title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  height: 96rpx;
  border-bottom: 1px solid #EAEBEC;
}

.title-left {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-title {
  width: 40rpx;
  height: 40rpx;
  margin-right: 12rpx;
  background-size: 100% 100%;
}

.title-content {
  height: 48rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #262626;
  line-height: 48rpx;

}

.no-reserve {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 484rpx;
}

.no-reserve-img {
  width: 212rpx;
  height: 230rpx;
  margin-top: 30rpx;
  margin-bottom: 24rpx;
}

.no-reserve-content {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
}

.btn {
  width: 642rpx !important;
  height: 88rpx !important;
  padding: 0;
  background: #fff;
  border-radius: 8rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  line-height: 88rpx;
}

.btn-reserve {
  margin-top: 48rpx;
  border: 1px solid #054CFF;
  color: #054CFF;
}

.btn-logout {
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  border: 1px solid #FA5175;
  color: #FA5175;
}