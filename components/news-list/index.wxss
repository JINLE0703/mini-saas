.card-container {
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.no-more {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 96rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 96rpx;
}

.no-more::before {
  content: '';
  width: 40rpx;
  height: 1px;
  margin-right: 12rpx;
  background-color: #D0D0D0;
}

.no-more::after {
  content: '';
  width: 40rpx;
  height: 1px;
  margin-left: 12rpx;
  background-color: #D0D0D0;
}